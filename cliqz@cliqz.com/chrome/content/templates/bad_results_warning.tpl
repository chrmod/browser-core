<div class="cqz-message-bar">
  <div class="cqz-message cqz-message-alert">
    {{local 'OutOfCoverageWarning'}}
  </div>
  <div style="float:right" cliqz-action="disable-cliqz">
    <span class="cqz-btn cqz-btn-success" state="keep-cliqz">{{local 'keep-cliqz'}}</span>
    <span class="cqz-btn cqz-btn-error" state="disable-cliqz">{{local 'disable-cliqz' }}</span>

  </div>
</div>
