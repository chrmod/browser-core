<a class="historyLink" data-index="{{index}}" target="_blank" href="{{model.url}}" title="{{model.title}}">
  <div class="cliqz-brand-logo" style="{{model.logo.style}}">{{model.logo.text}}</div>
  <div class="title">{{model.displayTitle}}</div>
  <button class="close" {{action "remove"}}>X</button>
</a>
