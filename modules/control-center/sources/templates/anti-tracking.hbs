      <!--end .accordion-section-->
      <div class="setting-accordion-section">
        <a class="setting-accordion-section-title" href="#accordion-2">
          <svg id="arrow" width="13px" height="13px" viewBox="1017 657 13 13" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
            <defs></defs>
            <polygon id="Rectangle" stroke="none" fill="#838383" fill-rule="evenodd" transform="translate(1023.344411, 663.208565) rotate(45.000000) translate(-1023.344411, -663.208565) " points="1018.95691 660.49484 1027.73191 658.821065 1026.05814 667.596065"></polygon>
          </svg>
          <span>Test 2</span>
        </a>
        <div id="accordion-2" class="setting-accordion-section-content">
          <span class="bullet">TTTWWWOOOOOOOO</span>
        </div>
        <!--end .accordion-section-content-->
      </div>
      <!--end .accordion-section-->
      <div class="setting-accordion-section">
        <a class="setting-accordion-section-title" href="#accordion-3">
          <svg id="arrow" width="13px" height="13px" viewBox="1017 657 13 13" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
            <defs></defs>
            <polygon id="Rectangle-546-Copy-11" stroke="none" fill="#838383" fill-rule="evenodd" transform="translate(1023.344411, 663.208565) rotate(45.000000) translate(-1023.344411, -663.208565) " points="1018.95691 660.49484 1027.73191 658.821065 1026.05814 667.596065"></polygon>
          </svg>
          <span>Test 3</span>
        </a>
        <div id="accordion-3" class="setting-accordion-section-content">
          <span class="bullet">TTTHHHREEEEEE</span>
        </div>
        <!--end .accordion-section-content-->
      </div>
      <!--end .accordion-section-->
    </div>
    <!--end .accordion-->

  </div>
  <div id="bottom-part">
    <div class="description" data-i18n="control-center-datapoints"></div>
    <div class="switches" class="switches-antitrack" active="true">
      <!--======= Switch ======-->
      <span>
        <span class="active cqz-switch">
          <span class="cqz-switch-box"></span>
        </span>
        <span class="active" data-i18n="control-center-switch-on"></span>
      </span>
    </div>
    <div class="counter">
      <!--======= Shield =======-->
      <svg id="shield" width="42px" height="50px" viewBox="-1 0 38 44" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <defs></defs>
        <path class="fill" d="M18.0059,0.375 C18.0059,0.375 13.1119,9.034 1.3039,9.034 C0.8799,9.034 0.4479,9.023 0.0059,9 L0.0049,9 C-0.1111,8.998 1.6699,34.772 18.0059,44 C34.3409,34.773 36.1219,9 36.0069,9 L36.0059,9 C35.5649,9.023 35.1319,9.034 34.7079,9.034 C22.8979,9.034 18.0059,0.375 18.0059,0.375 M32.4389,12.139 C31.4659,18.564 28.1719,33.281 18.0059,40.233 C7.8339,33.279 4.5449,18.563 3.5729,12.139 C10.6619,11.534 15.3369,8.172 18.0059,5.47 C20.6739,8.172 25.3499,11.534 32.4389,12.139 Z"
