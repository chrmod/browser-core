{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "commonjs": false,
        "es6": true,
        "mocha": true,
        "node": true,
        "shared-node-browser": true,
        "webextensions": true,
        "worker": true
    },
    "globals": {
        "CLIQZEnvironment": {},
        "CliqzABTests": {},
        "CliqzEvents": {},
        "CliqzHandlebars": {},
        "CliqzHistoryCluster": {},
        "CliqzHistoryManager": {},
        "CliqzLanguage": {},
        "CliqzSearchHistory": {},
        "CliqzUtils": {},
        "Components": {},
        "Services": {},
        "dump": {},
        "XPCOMUtils": {}
    },
    "rules": {
        "import/no-unresolved": "off",
        "import/no-extraneous-dependencies": 0,
        "import/extensions": "off",
        "class-methods-use-this": "off",
        "no-restricted-syntax": [
            'error',
            'ForInStatement',
            'LabeledStatement',
            'WithStatement',
        ]
    }
}
