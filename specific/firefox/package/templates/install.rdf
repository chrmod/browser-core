<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">
		<em:id>cliqz@cliqz.com</em:id>
		<em:name>{{ name }}</em:name>
		<em:version>{{ version }}</em:version>

		<em:type>2</em:type>
		<em:creator>feedback@cliqz.com</em:creator>
		<em:bootstrap>true</em:bootstrap>
    <em:multiprocessCompatible>true</em:multiprocessCompatible>

		{% if amo != 'True' %}
		{# Not working with CDN. It needs to be a https but our certificate is broken on the CDN. #}
		{% if beta == 'True' %}
		<em:updateURL>https://s3.amazonaws.com/cdncliqz/update/beta/latest.rdf</em:updateURL>
		{% else %}
		<em:updateURL>https://s3.amazonaws.com/cdncliqz/update/browser/latest.rdf</em:updateURL>
		{% endif %}
		{% endif %}

		<em:description>
		</em:description>
		<em:homepageURL>http://cliqz.com/</em:homepageURL>
		<em:targetApplication>

			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>29.0</em:minVersion>
				<em:maxVersion>45.*</em:maxVersion>

			</Description>
		</em:targetApplication>
	</Description>
</RDF>
